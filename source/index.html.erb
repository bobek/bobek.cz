      <span class="faded date"><%= article.date.strftime('%b %e') %></span>
      <strong><%= article.title %></strong>
      <div class="faded"><%= article.data.subtitle%></div>
    </a>
  <% end %>
  <a href="/blog" class="row gap-2">
    <span class="faded date">&nbsp;</span>
    <strong>More Posts...</strong>
  </a>
</div>

